	<script type="text/javascript">
	skrollr.init({
		smoothScrolling: true,
		edgeStrategy: 'set',
		forceHeight: true,
		easing: {
			vibrate: function(p) {
				return Math.sin(p * 10 * Math.PI);
			}
		}
	});
	</script>
